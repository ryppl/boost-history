template <typename number> struct test_from_string_small
{
	void run()
	{
		for (int i = 0; i < 1000000; ++i)
		{
			number a("23943");
		}
	}
};

template <typename number> struct test_from_string_medium
{
	void run()
	{
		for (int i = 0; i < 200; ++i)
		{
			number a("");
		}
	}
};
