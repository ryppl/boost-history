project cgi.examples.hello_world ;

exe scgi_hw : main.cpp ;

# Our install rule (builds binaries and copies them to <location>)
install install
  :
    scgi_hw
  :
    <location>$(scgi-bin)
  ;

# Only install example if you use `bjam install' or equivalent
explicit install ;
